<!DOCTYPE html>
<!--
    Henry Matsen
    Sec 3
    Mission 04
    Desc: A site about me. Includes a grade calculator that takes the percentage of each type of grade and calculates the final grade
-->
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <!-- bootstrap css -->
    <!-- #8 style the site using bootstrap-->
    <link href="~/lib/bootstrap/css/bootstrap.min.css" rel="stylesheet" />
    <!-- personal stylesheet -->
    <!-- #9 Include at least one change to the styling using your own CSS file-->
    <link href="~/css/Styles.css" rel="stylesheet" />
    <!-- external stylesheet (so hero img goes all the way out to the border)-->
    <link rel=“stylesheet” href=“https://cdnjs.cloudflare.com/ajax/libs/normalize/8.0.1/normalize.min.css” integrity=“sha512-NhSC1YmyruXifcj/KFRWoC561YpHpc5Jtzgvbuzx5VozKpWvQ+4nXhPdFgmx8xqexRcpAglTj9sIBWINXa8x5w = =” crossorigin=“anonymous” referrerpolicy=“no-referrer” />
    <!-- Favicons -->
    <!-- #7 An icon for the website-->
    <link rel="apple-touch-icon" sizes="180x180" href="favicon/apple-touch-icon.png">
    <link rel="icon" type="image/png" sizes="32x32" href="favicon/favicon-32x32.png">
    <link rel="icon" type="image/png" sizes="16x16" href="favicon/favicon-16x16.png">
    <link rel="manifest" href="favicon/site.webmanifest">
    <link rel="mask-icon" href="favicon/safari-pinned-tab.svg" color="#5bbad5">
    <link rel="shortcut icon" href="favicon/favicon.ico">
    <meta name="msapplication-TileColor" content="#da532c">
    <meta name="msapplication-config" content="favicon/browserconfig.xml">
    <meta name="theme-color" content="#ffffff">
    <!-- #1 A webpage title-->
    <title>@ViewData["Title"]</title>
</head>
<body>
    <nav>
        <ul class="main-menu">
            <li><a asp-controller="Home" asp-action="Index">Home</a></li>
            <li><a asp-controller="Home" asp-action="Calculator">Grade Calculator</a></li>
        </ul>
    </nav>

    <div>
        @RenderBody()
    </div>
    <!--import jquery-->
    <script src="~/lib/jquery/jquery-3.6.3.min.js"></script>
    <!--bootstrap js-->
    <script src="~/lib/bootstrap/js/bootstrap.min.js"></script>
    <!--js for the calculator-->
    <script src="~/js/Calculator.js"></script>
</body>
</html>
